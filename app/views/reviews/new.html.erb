<div class="container">
  <h3>We welcome your feedback!</h3>
  <%= form_for [@membership, @review] do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :feedback %>
      <%= f.text_field :feedback %>
      <%= f.submit "submit", class: "btn btn-default" %>
    </div>
  <% end %>
</div>
