<div class="container">
  <div class="row nav-bar" >
    <div class="col-sm-3">
      <p><%= link_to 'HOME', root_path %></p>
    </div>
    <div class="col-sm-3">
      <p><%= link_to 'MEMBERSHIPS', memberships_path %></p>
    </div>
    <div class="col-sm-3">
      <p>GALLERY</p>
    </div>
    <div class="col-sm-3">
      <p>CONTACT</p>
    </div>
  </div>
  <br>
  <div class="row form-row">
    <div class="col-lg-6">
      <h3>We welcome your feedback!</h3>
      <br>
      <h4>Review <%= @membership.membership_type %></h4>
      <%= form_for [@membership, @review] do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div class="form-group">
        <%= f.label :feedback %>
        <%= f.text_field :feedback %>
      </div>
      <div class="form-group">
        <%= f.submit "submit", class: "btn btn-default" %>
      </div>
      <% end %>
    </div>
    <div class="col-lg-6">
      <img src="/assets/form.png">
    </div>
  </div>
</div>
